{
	"compilerOptions": {
		"lib": [
			"ESNext",
			"DOM"
		],
		"target": "ESNext",
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "Node",
		"strict": true,
		"module": "ES6",
		"allowJs": true,
		"outDir": "./dist/",
		"newLine": "crlf",
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"types": ["solid-js"]
	},
}